<div class="row">
    <div class="col-9">
        <h2 class="text-primary">User List</h2>
    </div>
    <div class="col-3">
        <a asp-page="/Account/Register" asp-area="Identity" class="btn btn-info form-control">
            <i class="fa-solid fa-plus"></i>New User &nbsp;
            </a>
    </div>
</div>
<div class="container rounded border p-2 m-2">
    <table id="tbldata" class="table table-bordered tabletable-striped table-active">
        <thead>
          <tr>
              <th>Name</th>
              <th>Email</th>
              <th>Phone Number</th>
              <th>Company</th>
              <th>Role</th>
              <th>Lock User</th>
          </tr>
        </thead>
        <tbody></tbody>
    </table>
</div>
@section Scripts{
    <script src="~/js/User.js"></script>
}